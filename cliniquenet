@-webkit-keyframes scaleDisappear {
	0% {
		opacity: 1;
		-webkit-transform: scale(1);
	}
	60% {
		opacity: 0;
		-webkit-transform: scale(1.3);
	}
	100% {
		opacity: 1;
		-webkit-transform: scale(1);
	}
}
